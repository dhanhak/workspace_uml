package e05_assoclation_aggregation;

public class Human {
	private Heart heart = new Heart();
	private Eyes eyes = new Eyes();

	public Human() {
		super();
	}

	public Heart getHeart() {
		return heart;
	}

	public void setHeart(Heart heart) {
		this.heart = heart;
	}

	public Eyes getEyes() {
		return eyes;
	}

	public void setEyes(Eyes eyes) {
		this.eyes = eyes;
	}

}
